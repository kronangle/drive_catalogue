<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="panePrimary" alignment="CENTER" prefHeight="248.0" prefWidth="298.0" spacing="20.0" styleClass="custom-pane" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clearanglestudios.drive_catalogue.PrimaryController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/clear_angle_logo_icon_grey.png" />
         </image>
      </ImageView>
      <Button fx:id="assignButton_Primary" mnemonicParsing="false" onAction="#switchToAssign" prefWidth="63.0" styleClass="custom-button" text="ASSIGN" />
      <Button fx:id="ingestButton_Primary" mnemonicParsing="false" onAction="#switchToIngest" prefWidth="63.0" styleClass="custom-button" text="INGEST" />
      <Button fx:id="returnButton_Primary" mnemonicParsing="false" onAction="#switchToReturn" prefWidth="63.0" styleClass="custom-button" text="RETURN" />
      <Button fx:id="queryButton_Primary" mnemonicParsing="false" onAction="#switchToQuery" prefWidth="63.0" styleClass="custom-button" text="QUERY" />
      <Button fx:id="logoutButton_Primary" mnemonicParsing="false" onAction="#logoutButtonClicked" styleClass="custom-button" text="LOGOUT" />
      <Label fx:id="loggedInLabel_primary" styleClass="custom-label" text="Logged in as: " textFill="WHITE" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
